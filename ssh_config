Host bastionNSO
  User ubuntu
  HostName 103.57.72.242
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile=~/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  ForwardAgent yes

Host HAproxy
  User ubuntu
  HostName 103.57.72.206
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile=~/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no

Host devA
  User ubuntu
  HostName 10.1.0.21
  StrictHostKeyChecking no
  PasswordAuthentication no
  ProxyJump bastionNSO

